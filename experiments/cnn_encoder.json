{
    "vocabulary": {
        "min_count": {"tokens": 5}
    },
    "dataset_reader": {
        "type": "mhd",
        "token_indexers": {
            "tokens": {
                "type": "single_id",
                "lowercase_tokens": true
            }
        }
    },
    "train_data_path": "/home/rlogan/projects/pcori/preprocessing/data/clean.train.jsonl",
    "validation_data_path": "/home/rlogan/projects/pcori/preprocessing/data/clean.test.jsonl",
    "model": {
        "type": "crf_session_tagger",
        "dropout": 0.5,
        "text_field_embedder": {
            "tokens": {
                "type": "embedding",
                "embedding_dim": 200,
                "trainable": true
            }
        },
        "inner_encoder": {
            "type": "lstm",
            "input_size": 200,
            "hidden_size": 128,
            "dropout": 0.50,
            "num_layers": 2,
            "bidirectional": true
        },
        "outer_encoder": {
            "type": "lstm",
            "input_size": 256,
            "hidden_size":64,
            "dropout": 0.5,
            "num_layers": 1,
            "bidirectional": true
        }
    },
    "iterator": {"type": "basic", "batch_size": 2},
    "trainer": {
        "optimizer": "adam",
        "num_epochs": 50,
        "cuda_device": 0
    }
}
